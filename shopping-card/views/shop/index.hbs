{{# each products }}
<div class="row">
  {{# each this }}
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{this.imageSrc}}" alt="..." class="image-responsive">
      <div class="caption">
        <h3>"{{this.title}}"</h3>
        <p class="description">{{this.description}}</p>
        <div class="clearfix">
          <span class="price pull-left"> {{this.price}} pln </span>
          <a href="/add-to-card/{{this._id}}" class="btn btn-primary" style="float: right" role="button">Add to shopping card</a>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>
{{/each}}